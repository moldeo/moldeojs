{
  "name": "MoldeoJS",
  "version": "0.0.1",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "start": "ng serve --port 8888",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build && cp -R src/assets/molrepos dist/molrepos && cp -R src/assets/data dist/data",
    "prepare": "cp -a electron/. dist/",
    "electron": "npm run prepare && electron ./dist",
    "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist App --platform=linux --arch=all --out=dist-app --overwrite",
    "buildelectron": "npm run build && npm run electron",
    "buildsite": "ng build -prod --build-optimizer --base-href . && cp -R src/assets/molrepos dist/molrepos && cp -R src/assets/data dist/data",
    "buildodoo": "ng build --base-href / && cp -R src/assets/molrepos dist/molrepos && cp -R src/assets/data dist/data"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "8.3.22",
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/webgl2": "0.0.5",
    "@types/webrtc": "0.0.25",
    "bootstrap": "^3.3.7",
    "core-js": "^3.6.4",
    "mathjs": "^6.5.0",
    "ngx-bootstrap": "^5.3.2",
    "node-osc": "^4.1.4",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "three": "0.110.0",
    "three-trackballcontrols": "0.0.8",
    "ts-helpers": "^1.1.2",
    "tslib": "^1.10.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular/cli": "^8.3.22",
    "@types/jasmine": "3.5.0",
    "@types/mathjs": "6.0.3",
    "@types/node": "^13.1.6",
    "@types/three": "^0.103.2",
    "codelyzer": "^5.0.1",
    "electron": "^7.2.4",
    "electron-packager": "^14.2.0",
    "jasmine": "^3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.6.2",
    "tslint": "^5.20.1",
    "typescript": "3.5.3",
    "webdriver-manager": "12.1.7"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
